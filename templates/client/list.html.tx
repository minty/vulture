[% WRAPPER "_page.tx" WITH body_class = 'index', type = 'index' %]
<h1>client list</h1>
<ul class=clientlist>
  [% WHILE (client = clients.next) %]
    <li
      class=active
      data-id="[% client.id %]"
      data-guid="[% client.guid %]"
      data-sessionid="[% client.sessionid %]"
      data-agent="[% client.agent %]"
    >[% client.guid %] / [% client.sessionid %] : [% client.agent %]</li>
  [% END %]
</ul>
[% END %]
